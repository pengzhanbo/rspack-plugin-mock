{
  "name": "rspack-plugin-mock",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@9.6.0",
  "description": "inject api mock server to development server",
  "author": "pengzhanbo <q942450674@outlook.com> (https://github.com/pengzhanbo)",
  "license": "MIT",
  "keywords": [
    "mock",
    "mock server",
    "rspack",
    "rsbuild",
    "rspack-plugin",
    "rsbuild-plugin"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      },
      "require": {
        "types": "./dist/server.d.cts",
        "default": "./dist/server.cjs"
      }
    },
    "./rsbuild": {
      "import": {
        "types": "./dist/rsbuild.d.ts",
        "default": "./dist/rsbuild.js"
      },
      "require": {
        "types": "./dist/rsbuild.d.cts",
        "default": "./dist/rsbuild.cjs"
      }
    },
    "./helper": {
      "import": {
        "types": "./dist/helper.d.ts",
        "default": "./dist/helper.js"
      },
      "require": {
        "types": "./dist/helper.d.cts",
        "default": "./dist/helper.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.20.0",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "tsup src --watch",
    "build": "tsup",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp package.json --execute=\"pnpm release:changelog\" --commit --all --push --tag && pnpm publish --access public"
  },
  "peerDependencies": {
    "@rsbuild/core": ">=0.7.0",
    "@rspack/core": ">=0.7.0"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    },
    "@rspack/core": {
      "optional": true
    }
  },
  "dependencies": {
    "@pengzhanbo/utils": "^1.1.2",
    "@rollup/pluginutils": "^5.1.0",
    "chokidar": "^3.6.0",
    "co-body": "^6.2.0",
    "cookies": "^0.9.1",
    "cors": "^2.8.5",
    "debug": "^4.3.6",
    "fast-glob": "^3.3.2",
    "formidable": "2.1.2",
    "http-status": "^1.7.4",
    "is-core-module": "^2.15.0",
    "json5": "^2.2.3",
    "memfs": "^4.11.1",
    "mime-types": "^2.1.35",
    "path-to-regexp": "^7.1.0",
    "picocolors": "^1.0.1",
    "portfinder": "^1.0.32",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^1.12.0",
    "@rsbuild/core": "1.0.1-beta.9",
    "@rspack/core": "1.0.0-beta.2",
    "@types/co-body": "^6.1.3",
    "@types/cookies": "^0.9.0",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/formidable": "2.0.6",
    "@types/is-core-module": "^2.2.2",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.1.0",
    "@types/ws": "^8.5.12",
    "bumpp": "^9.4.2",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.8.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
